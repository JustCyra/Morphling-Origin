                {
                    "type": "origins:grant_power",
                    "power": "cyra:morphling/6_end/temp/lore",
                    "source": "*:*"
                }
            ]
        }
    },
    "count_down": {
        "type": "origins:action_over_time",
        "interval": 10,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:relative_health",
                    "comparison": ">",
                    "compare_to": 0.0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_progress",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_progress",
            "operation": "add",
            "change": -1
        }
    },
    "finish_on_respawn": {
        "type": "origins:action_on_callback",
        "entity_action_respawned": {
            "type": "origins:delay",
            "ticks": 1,
            "action": {
                "type": "origins:set_resource",
                "resource": "*:*_progress",
                "value": 0
            }
        }
    },
    "on_potion_drinking": {
        "type": "origins:action_over_time",
        "interval": 10,
        "condition": {
            "type": "origins:and",
