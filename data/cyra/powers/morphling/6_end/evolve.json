                    "count": 250,
                    "speed": 0.5,
                    "force": false,
                    "offset_y": 0.0,
                    "spread": {
                        "x": 1.0,
                        "y": 0.5,
                        "z": 1.0
                    }
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.respawn_anchor.set_spawn",
