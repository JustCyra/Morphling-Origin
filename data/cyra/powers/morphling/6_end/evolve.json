                        {
                            "effect": "minecraft:blindness",
                            "duration": 100,
                            "amplifier": 1,
                            "is_ambient": false,
                            "show_particles": false,
                            "show_icon": false
                        },
                        {
                            "effect": "minecraft:slowness",
                            "duration": 100,
                            "amplifier": 1,
