{
    "type": "origins:multiple",
    "name": "Unstable Waterborn",
    "description": "The End corrupted you. You are vulnerable to fire once again, and now water is new problem for you. However, the corruption has gifted you new unique reality displacement abilities.",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/info.png",
            "text": {
                "text": "Form Stats:",
                "extra": [
                    {
                        "text": "",
                        "color": "green",
                        "extra": [
                            "\n+50% Water Swim Speed",
                            "\n+50% Water Visibility",
                            "\n-30% Hunger Usage",
                            "\n-30% Model Width",
                            "\n+1.0 Block Reach",
                            "\n+0.5 Attack Reach"
                        ]
                    },
                    {
                        "text": "",
                        "color": "red",
                        "extra": [
                            "\n-6 Max Health",
                            "\n+200% Fire Damage Taken",
                            "\nTake damage from water items and blocks"
                        ]
                    }
                ]
            }
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/arrow_up.png",
            "text": {
                "text": "Changes on ",
                "extra": [
                    {
                        "text": "[",
                        "color": "gray",
                        "extra": [
                            {
                                "text": "Morphling VII",
                                "color": "blue"
                            },
                            {
                                "text": "]"
                            }
                        ]
                    }
                ]
            }
        }
    ],
    "attributes": {
        "type": "origins:attribute",
        "update_health": true,
        "modifiers": [
            {
                "attribute": "additionalentityattributes:water_speed",
                "operation": "multiply_base",
                "value": 0.5
            },
            {
                "attribute": "additionalentityattributes:water_visibility",
                "operation": "multiply_base",
                "value": 0.5
            },
            {
                "attribute": "minecraft:generic.max_health",
                "operation": "addition",
                "value": -6
            },
            {
                "attribute": "reach-entity-attributes:attack_range",
                "operation": "addition",
                "value": 0.5
            },
            {
                "attribute": "reach-entity-attributes:reach",
                "operation": "addition",
                "value": 1.0
            }
        ]
    },
    "less_exhaust": {
        "type": "origins:modify_exhaustion",
        "modifier": {
            "operation": "multiply_base_additive",
            "value": -0.3
        }
    },
    "size": {
        "type": "origins:action_on_callback",
        "entity_action_added": {
            "type": "origins:delay",
            "ticks": 1,
            "action": {
                "type": "origins:execute_command",
                "command": "function cyra:morphling/size/forms/long"
            }
        },
        "entity_action_removed": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:or",
                "inverted": true,
                "conditions": [
                    {
                        "type": "origins:origin",
                        "origin": "cyra:morphling/5_end",
                        "layer": "origins:origin"
                    },
                    {
                        "type": "origins:origin",
                        "origin": "cyra:morphling/6_end",
                        "layer": "origins:origin"
                    },
                    {
                        "type": "origins:origin",
                        "origin": "cyra:morphling/7_combined/main",
                        "layer": "origins:origin"
                    }
                ]
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "function cyra:morphling/size/restore_to_default"
            }
        }
    },
    "fire_damage": {
        "type": "origins:modify_damage_taken",
        "damage_condition": {
            "type": "origins:fire"
        },
        "modifier": {
            "operation": "multiply_base_additive",
            "value": 2
        }
    }
}