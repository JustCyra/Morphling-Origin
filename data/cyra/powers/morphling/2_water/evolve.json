{
    "type": "origins:multiple",
    "hidden": true,
    "progress": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:spawn_particles",
                    "particle": {
                        "type": "minecraft:flame"
                    },
                    "count": 72,
                    "speed": 0.5,
                    "force": false,
                    "offset_y": 0.5,
                    "spread": {
                        "x": 0.15,
                        "y": 0.2,
                        "z": 0.15
                    }
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.respawn_anchor.set_spawn",
                    "volume": 1.0,
                    "pitch": 2.0
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:ui.toast.challenge_complete",
                    "volume": 1.0,
                    "pitch": 2.0
                },
                {
                    "type": "origins:execute_command",
                    "command": "advancement grant @s only cyra:morphling/progress/3_fire"
                }
            ]
        },
        "max_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:apply_effect",
                    "effects": [
                        {
                            "effect": "minecraft:darkness",
                            "duration": 100,
                            "amplifier": 1,
                            "is_ambient": false,
                            "show_particles": false,
                            "show_icon": false
                        },
                        {
                            "effect": "minecraft:slowness",
                            "duration": 100,
                            "amplifier": 1,
                            "is_ambient": false,
                            "show_particles": false,
                            "show_icon": false
                        }
                    ]
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.enchantment_table.use",
                    "volume": 1.0,
                    "pitch": 0.5
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.portal.trigger",
                    "volume": 1.0,
                    "pitch": 0.8
                },
                {
                    "type": "origins:grant_power",
                    "power": "cyra:morphling/2_water/temp/lore",
                    "source": "*:*"
                }
            ]
        }
    },
    "count_down": {
        "type": "origins:action_over_time",
        "interval": 10,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:relative_health",
                    "comparison": ">",
                    "compare_to": 0.0
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_progress",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_progress",
            "operation": "add",
            "change": -1
        }
    },
    "finish_on_respawn": {
        "type": "origins:action_on_callback",
        "entity_action_respawned": {
            "type": "origins:delay",
            "ticks": 1,
            "action": {
                "type": "origins:set_resource",
                "resource": "*:*_progress",
                "value": 0
            }
        }
    },
    "on_eating_rod": {
        "type": "apugli:edible_item",
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_progress",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:dimension",
                    "dimension": "minecraft:the_nether"
                }
            ]
        },
        "item_condition": {
            "type": "origins:ingredient",
            "ingredient": {
                "item": "minecraft:blaze_rod"
            }
        },
        "food_component": {
            "hunger": 1,
            "saturation": 1.0,
            "meat": false,
            "snack": false,
            "always_edible": true
        },
        "use_action": "eat",
        "sound": "minecraft:entity.generic.extinguish_fire",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:damage",
                    "amount": 2.0,
                    "source": {
                        "name": "inFire",
                        "bypasses_armor": true,
                        "unblockable": true,
                        "fire": true
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:relative_health",
                        "comparison": ">",
                        "compare_to": 0.0
                    },
                    "if_action": {
                        "type": "origins:set_resource",
                        "resource": "*:*_progress",
                        "value": 10
                    }
                }
            ]
        }
    }
}