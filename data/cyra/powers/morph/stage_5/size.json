{
    "type": "origins:multiple",
    "name": "Ever Changing",
    "description": "You ever so evolving, constantly changing and improving\nThe following advancement will finish your evolution:\n\n     \"A Furious Cocktail\"",
    "respawn_and_stuff": {
        "type": "origins:action_on_callback",
        "entity_action_added": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:delay",
                    "ticks": 10,
                    "action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:advancement",
                            "advancement": "cyra:morph/stages/fun_begins",
                            "inverted": true
                        },
                        "if_action": {
                            "type": "origins:execute_command",
                            "command": "advancement grant @s only cyra:morph/stages/fun_begins"
                        }
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:advancement",
                        "advancement": "minecraft:nether/all_potions"
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "advancement revoke @s only minecraft:nether/all_potions"
                    }
                },
                {
                    "type": "origins:delay",
                    "ticks": 6,
                    "action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "cyra:morph/stage_5/switch_main",
                            "comparison": "==",
                            "compare_to": 0
                        },
                        "if_action": {
                            "type": "origins:execute_command",
                            "command": "function cyra:morph/size_nether"
                        },
                        "else_action": {
                            "type": "origins:execute_command",
                            "command": "function cyra:morph/size_end"
                        }
                    }
                },
                {
                    "type": "origins:delay",
                    "ticks": 6,
                    "action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:gamemode",
                                    "gamemode": "spectator",
                                    "inverted": true
                                },
                                {
                                    "type": "origins:status_effect",
                                    "effect": "minecraft:invisibility",
                                    "inverted": true
                                }
                            ]
                        },
                        "if_action": {
                            "type": "origins:if_else",
                            "condition": {
                                "type": "origins:resource",
                                "resource": "cyra:morph/stage_5/switch_main",
                                "comparison": "==",
                                "compare_to": 0
                            },
                            "if_action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:delay",
                                        "ticks": 1,
                                        "action": {
                                            "type": "origins:execute_command",
                                            "command": "particle minecraft:flame ~ ~0.8 ~ 0.2 0.4 0.2 0 100"
                                        }
                                    },
                                    {
                                        "type": "origins:delay",
                                        "ticks": 10,
                                        "action": {
                                            "type": "origins:execute_command",
                                            "command": "particle minecraft:flame ~ ~0.8 ~ 0.2 0.4 0.2 1 100"
                                        }
                                    }
                                ]
                            },
                            "else_action": {
                                "type": "origins:delay",
                                "ticks": 1,
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "particle minecraft:reverse_portal ~ ~0.7 ~ 0 0 0 1 50"
                                }
                            }
                        }
                    }
                }
            ]
        },
        "entity_action_respawned": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:delay",
                    "ticks": 6,
                    "action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "cyra:morph/stage_5/switch_main",
                            "comparison": "==",
                            "compare_to": 0
                        },
                        "if_action": {
                            "type": "origins:execute_command",
                            "command": "function cyra:morph/size_nether"
                        },
                        "else_action": {
                            "type": "origins:execute_command",
                            "command": "function cyra:morph/size_end"
                        }
                    }
                },
                {
                    "type": "origins:change_resource",
                    "resource": "cyra:morph/stage_5/teleport_bar",
                    "change": 1000,
                    "operation": "set"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "cyra:morph/stage_5/teleport_unstable",
                    "change": 0,
                    "operation": "set"
                }
            ]
        },
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "scale reset @s"
        }
    },
    "adv_fix": {
        "type": "origins:action_over_time",
        "interval": 100,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "advancement revoke @s only cyra:morph/life_given"
                },
                {
                    "type": "origins:execute_command",
                    "command": "advancement revoke @s only cyra:morph/return_to_fortress"
                },
                {
                    "type": "origins:execute_command",
                    "command": "advancement revoke @s only cyra:morph/the_last_snack"
                }
            ]
        },
        "condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:advancement",
                    "advancement": "cyra:morph/life_given"
                },
                {
                    "type": "origins:advancement",
                    "advancement": "cyra:morph/return_to_fortress"
                },
                {
                    "type": "origins:advancement",
                    "advancement": "cyra:morph/the_last_snack"
                }
            ]
        }
    }
}